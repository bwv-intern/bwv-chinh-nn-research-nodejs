{
  "compilerOptions": {
    "module": "NodeNext",
    "sourceMap": true,
    "moduleResolution": "NodeNext",
    "target": "ES2023",
    "outDir": "dist",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    }
  },
  // "files": ["src/type.d.ts"],
  "include": ["src/", "environment.d.ts"]
}
